
<h1>Contacts#new</h1>

<%= form_with model: @contact do |f| %>
  <%= f.label :request_user_id %>
  <%= f.text_field :request_user_id %>
<br>
  <%= f.label :shop_id %>
  <%= f.text_field :shop_id %>
<br>
  <%= f.label :menu_id %>
  <%= f.text_field :menu_id %>
<br>
  <%= f.label :comment %>
  <%= f.text_field :comment %>
<br>
  <%= f.label :fee %>
  <%= f.text_field :fee %>
<br>
  <%= f.label :request_receive_time %>
  <%= f.text_field :request_receive_time %>
<br>
  <%= f.submit "申請する" %>
<% end %>